{
  "name": "Ticketer",
  "description": "Ticket tracking and productivity helper",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "alarms",
    "storage",
    "background"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "optional_permissions": [],
  "action": {
    "default_popup": "index.html",
    "default_title": "Ticketer"
  },
  "icons": {
    "16": "images/ticket.png",
    "24": "images/ticket_24.png",
    "32": "images/ticket_32.png",
    "64": "images/ticket_64.png",
    "128": "images/ticket_128.png",
    "256": "images/ticket_256.png"
  }
}